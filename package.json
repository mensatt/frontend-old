{
  "name": "neosigfood-frontend",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "yarn run lint:next && yarn run lint:style",
    "lint-fix": "yarn run lint:style-fix",
    "lint:next": "next lint",
    "lint:style": "stylelint \"src/**/*.scss\"",
    "lint:style-fix": "stylelint \"src/**/*.scss\" --fix",
    "types": "apollo client:codegen src/graphql/graphql-types.ts --endpoint='http://localhost:5858/graphql' --target typescript --outputFlat --includes='src/**/*.gql.ts'",
    "clean": "rm -rf node_modules/"
  },
  "dependencies": {
    "@apollo/client": "^3.5.8",
    "@babel/core": "^7.0.0-0",
    "@reduxjs/toolkit": "^1.7.0",
    "graphql": "^16.3.0",
    "next": "12.0.10",
    "next-pwa": "^5.4.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^7.2.6",
    "sass": "^1.49.7",
    "stylelint": "^14.5.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "17.0.14",
    "@types/react": "17.0.38",
    "dayjs": "^1.10.7",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
    "postcss-scss": "^4.0.3",
    "postcss": "^8.4.6",
    "typescript": "4.5.5"
  }
}
